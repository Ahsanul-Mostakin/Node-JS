<h2>Students List</h2>
<a href="/students/addOrEdit" class="btn btn-primary mb-3">Add New Student</a>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>{{this.email}}</td>
            <td>{{this.mobile}}</td>
            <td>{{this.city}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
